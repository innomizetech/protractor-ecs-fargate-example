    "configurations": [
        {
            "name": "Debug protractor",
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "args": [
                "${workspaceRoot}/dist/config.js"
